{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    ".idea": true,
    ".vscode": true,
    "**/node_modules": true,
    "**/.svelte-kit": true,
    ".github": true,
    "LICENSE": true,
    ".npmrc": true
  },
  "eslint.format.enable": true,
  "eslint.validate": [
      "javascript", "javascriptreact", "typescript", "typescriptreact", "svelte"
  ],
  "eslint.probe": [
      "javascript",
      "javascriptreact",
      "typescript",
      "typescriptreact",
      "html",
      "markdown",
      "svelte"
  ],
  "files.eol": "\n",
  "files.trimTrailingWhitespace": true,
  "[svelte]": {
      "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[javascript]": {
      "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
}