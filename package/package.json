{
  "name": "moderndash",
  "type": "module",
  "description": "A lodash inspired utility framework for ESM/Typescript/ESNext",
  "sideEffects": false,
  "scripts": {
    "build": "ctix create ----startAt src --overwrite --noBackup & tsup",
    "prepublishOnly": "npm run build && @powershell cp '../README.md' './README.md' && @powershell cp '../LICENSE' './LICENSE'",
    "postpublish": "@powershell rm './README.md' && @powershell rm './LICENSE'",
    "test": "vitest",
    "test-ui": "vitest --ui"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Maggi64/moderndash.git"
  },
  "keywords": [
    "lodash",
    "utility",
    "helper",
    "underscore",
    "esm",
    "typescript"
  ],
  "author": "Maggi64",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Maggi64/moderndash/issues"
  },
  "browserslist": [
    ">2% and not dead"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "homepage": "https://moderndash.io",
  "devDependencies": {
    "@vitest/coverage-c8": "0.28.3",
    "@vitest/ui": "0.28.3",
    "vitest": "0.28.3",
    "tsup": "6.5.0",
    "ctix": "1.8.2"
  }
}
