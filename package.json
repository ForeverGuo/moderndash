{
    "name": "moderndash-monorepo",
    "version": "1.0.0",
    "private": true,
    "workspaces": [
        "benchmark",
        "website",
        "package"
    ],
    "type": "module",
    "scripts": {
        "build:docs-json": "typedoc --options typedoc.json && typedoc-json-parser --json ./website/src/assets/extractedTypes.json --verbose",
        "build:docs": "npm run build --workspace=package && npm run build:docs-json && npm run build --workspace=website",
        "start:docs": "cd website && npm run start"
    },
    "browserslist": [
        ">2% and not dead"
    ],
    "devDependencies": {
        "@types/showdown": "2.0.0",
        "@typescript-eslint/eslint-plugin": "5.52.0",
        "@typescript-eslint/parser": "5.52.0",
        "eslint": "8.34.0",
        "eslint-import-resolver-typescript": "3.5.3",
        "eslint-plugin-compat": "4.1.2",
        "eslint-plugin-deprecate": "0.7.0",
        "eslint-plugin-import": "2.27.5",
        "eslint-plugin-sonarjs": "0.18.0",
        "eslint-plugin-svelte": "2.19.0",
        "eslint-plugin-unicorn": "45.0.2",
        "typedoc": "0.23.25",
        "typescript": "4.9.5"
    }
}
