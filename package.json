{
    "name": "moderndash-monorepo",
    "version": "1.0.0",
    "private": true,
    "workspaces": [
        "benchmark",
        "website",
        "package"
    ],
    "engines": {
        "node": ">=18.x.x",
        "npm": ">=8.x.x"
    },
    "type": "module",
    "scripts": {
        "build:docs-json": "typedoc --options typedoc.json && typedoc-json-parser --json ./website/src/assets/extractedTypes.json --verbose",
        "build:docs": "npm run build:docs-json && cd website && npm run build",
        "start:docs": "cd website && npm run start"
    },
    "browserslist": [
        ">2% and not dead"
    ],
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "5.49.0",
        "@typescript-eslint/parser": "5.49.0",
        "eslint": "8.32.0",
        "eslint-import-resolver-typescript": "3.5.3",
        "eslint-plugin-compat": "4.0.2",
        "eslint-plugin-deprecate": "0.7.0",
        "eslint-plugin-import": "2.27.5",
        "eslint-plugin-sonarjs": "0.18.0",
        "eslint-plugin-svelte": "2.15.0",
        "eslint-plugin-unicorn": "45.0.2",
        "typedoc": "0.23.24",
        "typescript": "4.9.4"
    }
}
