{
  "name": "moderndash",
  "version": "0.0.5",
  "type": "module",
  "description": "A lodash rewrite for ESM/Typescript/ES2020",
  "scripts": {
    "build": "ctix create ----startAt src --overwrite --noBackup & tsc & tsc-alias",
    "test": "vitest",
    "test-ui": "vitest --ui",
    "playground": "npm run build & node playground"
  },
  "engines": {
    "node": ">=18.x.x",
    "npm": ">=8.x.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Maggi64/esmdash.git"
  },
  "keywords": [
    "lodash",
    "utility",
    "helper",
    "underscore"
  ],
  "author": "Maggi64",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Maggi64/esmdash/issues"
  },
  "browserslist": [
    ">2% and not dead"
  ],
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/"
  },
  "homepage": "https://github.com/Maggi64/esmdash#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.46.1",
    "@typescript-eslint/parser": "5.46.1",
    "@vitest/ui": "0.25.8",
    "eslint": "8.29.0",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-compat": "4.0.2",
    "eslint-plugin-deprecate": "0.7.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-sonarjs": "0.17.0",
    "eslint-plugin-unicorn": "45.0.2",
    "tsc-alias": "1.8.2",
    "typedoc": "0.23.22",
    "typescript": "4.9.4",
    "vitest": "0.25.8",
    "ctix": "1.8.0"
  }
}
